package com.arhat.chattest.model;

/**
 * Created by lihanguang on 2016/10/3.
 */
public class NewFriend {
    private String id;
    private String name;
    private int type;
    private String iconUrl;
    private String detail;
    private String from;
    public final static int ACCEPTED = 1;//已接受
    public final static int NOACCEPTED = 2;//未接受

    public NewFriend(String id, String name, int type, String iconUrl, String detail, String from) {
        this.id = id;
        this.name = name;
        this.type = type;
        this.iconUrl = iconUrl;
        this.detail = detail;
        this.from = from;
    }

    public NewFriend() {
    }

    public String getFrom() {
        return from;
    }

    public void setFrom(String from) {
        this.from = from;
    }

    public String getDetail() {
        return detail;
    }

    public void setDetail(String detail) {
        this.detail = detail;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }

    public String getIconUrl() {
        return iconUrl;
    }

    public void setIconUrl(String iconUrl) {
        this.iconUrl = iconUrl;
    }
}
